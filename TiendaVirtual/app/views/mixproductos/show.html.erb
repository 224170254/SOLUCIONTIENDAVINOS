<%- model_class = Mixproducto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:idmaestro) %>:</strong></dt>
  <dd><%= @mixproducto.idmaestro %></dd>
  <dt><strong><%= model_class.human_attribute_name(:stock) %>:</strong></dt>
  <dd><%= @mixproducto.stock %></dd>
  <dt><strong><%= model_class.human_attribute_name(:precio) %>:</strong></dt>
  <dd><%= @mixproducto.precio %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idformato) %>:</strong></dt>
  <dd><%= @mixproducto.idformato %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idformatoventa) %>:</strong></dt>
  <dd><%= @mixproducto.idformatoventa %></dd>
  <dt><strong><%= model_class.human_attribute_name(:idbodega) %>:</strong></dt>
  <dd><%= @mixproducto.idbodega %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              mixproductos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_mixproducto_path(@mixproducto), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              mixproducto_path(@mixproducto),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
