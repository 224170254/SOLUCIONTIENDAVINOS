<%- model_class = Mixproducto -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:idmaestro) %></th>
      <th><%= model_class.human_attribute_name(:stock) %></th>
      <th><%= model_class.human_attribute_name(:precio) %></th>
      <th><%= model_class.human_attribute_name(:idformato) %></th>
      <th><%= model_class.human_attribute_name(:idformatoventa) %></th>
      <th><%= model_class.human_attribute_name(:idbodega) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @mixproductos.each do |mixproducto| %>
      <tr>
        <td><%= link_to mixproducto.id, mixproducto_path(mixproducto) %></td>
        <td><%= mixproducto.idmaestro %></td>
        <td><%= mixproducto.stock %></td>
        <td><%= mixproducto.precio %></td>
        <td><%= mixproducto.idformato %></td>
        <td><%= mixproducto.idformatoventa %></td>
        <td><%= mixproducto.idbodega %></td>
        <td><%=l mixproducto.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_mixproducto_path(mixproducto), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      mixproducto_path(mixproducto),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_mixproducto_path,
            :class => 'btn btn-primary' %>
