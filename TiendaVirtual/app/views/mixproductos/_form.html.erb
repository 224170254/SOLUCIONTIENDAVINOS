<%= form_for @mixproducto, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :idmaestro, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :idmaestro, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :stock, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :stock, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :precio, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :precio, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :idformato, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :idformato, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :idformatoventa, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :idformatoventa, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :idbodega, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :idbodega, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                mixproductos_path, :class => 'btn' %>
  </div>
<% end %>
